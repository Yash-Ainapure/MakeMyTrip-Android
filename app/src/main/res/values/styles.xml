<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    
    <style name="circular">
        <item name="cornerSize">8%</item>
    </style>
    <style name="round">
        <item name="cornerSize">50%</item>
    </style>

    <style name="UPIButtonStyle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
